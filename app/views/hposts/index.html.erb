<p id="notice"><%= notice %></p>

<h1>Listing Hposts</h1>

<table>
  <thead>
    <tr>
      <th>Htmltag</th>
      <th>Picture</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hposts.each do |hpost| %>
      <tr>
        <td><%= hpost.id %></td>
        <td><%= link_to 'Show', hpost %></td>
        <td><%= link_to 'Edit', edit_hpost_path(hpost) %></td>
        <td><%= link_to 'Destroy', hpost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

